<%- include('./partials/header.ejs') %>

<main class="story">
    <div class="wrecker"></div> 
    <%- ctx.prismicH.asHTML(storyData.title) %>
    <%- ctx.prismicH.asHTML(storyData.content) %>

    <div class="buttonHolder" >

        <% if(previous){ %>
            <btn class="button back" type="button">
                <a href= <%-previous.url%>>Previous</a>
            </btn>
        <% } %>
        <% if(next){ %>
            <btn class="button next" type="button">
                <a href= <%-next.url%>>Next</a>
            </btn>
        <% } %>
    </div>
</main>

<script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.9.1/gsap.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.9.1/ScrollTrigger.min.js"></script>
<script src="/js/scrollingStory.js"></script>

